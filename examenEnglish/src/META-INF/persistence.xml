  <persistence-unit name="examenEnglishPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>Logica.Pregunta</class>
    <class>Logica.Opcion</class>
    <class>Logica.Examen</class>
    <class>Logica.ExamenXPregunta</class>
    <class>Logica.Persona</class>
    <class>Logica.RespuestaXExamen</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:postgresql://localhost:5432/multimedia"/>
      <property name="javax.persistence.jdbc.password" value="multimedia"/>
      <property name="javax.persistence.jdbc.driver" value="org.postgresql.Driver"/>
      <property name="javax.persistence.jdbc.user" value="multimedia"/>
    </properties>
  </persistence-unit>
</persistence>
